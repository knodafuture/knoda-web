<div class="row contest-item" style="border: 1px solid #dddddd; border-top: 0px;background-color: #fff;" data-contest-id="<%= contest.id %>">
  <div class="col-xs-12 center">
      <div class="row"><div class="col-xs-12">
         <img class="img-circle" src="http://aengland.s3.amazonaws.com/users/avatars/000/000/001/small/avatar_5_2x.png?1406318499">
         <h2><%= contest.name %></h2>
         <p class="description"><%= contest.description %></p>
      </div></div>
      <div class="row">
        <div class="col-xs-6">
          <% if my_contest_rank(contest) != nil %>
          <p class="stat-value"><%= my_contest_rank(contest) %></p>
          <p class="stat-label">overall(<%= contest.participants %>)</p>
          <% end %>
        </div>
        <div class="col-xs-6">
          <% if contest.leader_info %>
          <p class="stat-value"><%= contest.leader_info[:username] %></p>
          <p class="stat-label">current contest leader</p>
          <% end %>
        </div>
  </div>
</div>
</div>
