<% @contest.contest_stages.each do |stage| %>
<div class="panel panel-default predictionContainer">
    <div class="panel-heading" style="padding:0px 15px; background-color: #EFEFEF">
    <div class="row userInfo">
          <span class="username"><%= stage.name %></span>
    </div>
    <div class="row predictionInfo" style="background-color:white; padding: 8px 20px;">
      <p style="width:100%;display:inline-block;"><%= stage.sort_order %></p>
    </div>
  </div>
</div>
<% end %>
