<div class="col-xs-12">
<div class="row" style="border-bottom: 1px solid #efefef">
	<div class="col-xs-6">
		<h3>Agree</h3>
	</div>
	<div class="col-xs-6 right">
		<h3>Disagree</h3>
	</div>
</div>
<div class="row">
	<div class="col-xs-6">
		<% @agreeUsers.each do |i| %>
				<p><%= i.username %></p>
		<% end %>
	</div>
	<div class="col-xs-6 right">
		<% @disagreeUsers.each do |i| %>
				<p><%= i.username %></p>
		<% end %>			
	</div>
</div>
</div>